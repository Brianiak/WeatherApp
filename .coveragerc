[run]
source = src
omit =
    */tests/*
    */test_*.py
    setup.py
    */venv/*
    */virtualenv/*
    src/main.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
